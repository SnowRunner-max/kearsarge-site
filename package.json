    "svelte": "^4.2.19",
    "svelte-check": "^3.6.7",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
